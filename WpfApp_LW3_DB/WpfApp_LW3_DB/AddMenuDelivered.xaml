<Window x:Class="WpfApp_LW3_DB.AddMenuDelivered"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_LW3_DB"
        mc:Ignorable="d"
        Title="AddMenuDelivered" Height="450" Width="350">
    <Grid Margin="10">
        <StackPanel>
            <TextBlock Margin="0, 10, 0, 0">Farm ID</TextBlock>
            <TextBox x:Name="FarmIDTextBox"/>

            <TextBlock Margin="0, 10, 0, 0">Cultures ID</TextBlock>
            <TextBox x:Name="CulturesIDTextBox"/>

            <TextBlock Margin="0, 10, 0, 0">Order weight</TextBlock>
            <TextBox x:Name="OrderWeightTextBox"/>

            <TextBlock Margin="0, 10, 0, 0">Delivered weight</TextBlock>
            <TextBox x:Name="DeliveredWeightTextBox"/>

            <TextBlock Margin="0, 10, 0, 0">Yield farm</TextBlock>
            <TextBox x:Name="YieldFarmTextBox"/>

            <TextBlock Margin="0, 10, 0, 0">Product quality</TextBlock>
            <ComboBox x:Name="ProductQualityComboBox">
                <ComboBoxItem>first</ComboBoxItem>
                <ComboBoxItem Content="second"/>
                <ComboBoxItem Content="premium"/>
            </ComboBox>

            <TextBlock Margin="0, 10, 0, 0">Delivery date</TextBlock>
            <DatePicker x:Name="DeliveryDateDatePicker"/>

            <StackPanel Orientation="Horizontal" Margin="50, 0, 0, 0">
                <Button x:Name="OkButton" Content="OK" Width="100" Margin="10, 20, 0, 0" Click="OkButton_Click"/>
                <Button x:Name="CanceleButton" Content="Cancele" Width="100" Margin="10, 20, 0, 0"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
